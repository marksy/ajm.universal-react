{
  "name": "ajm.react-template",
  "version": "1.0.0",
  "scripts": {
    "depcheck": "depcheck --ignores prop-types,babel-core,babel-loader,babel-preset-env,css-loader,eslint-plugin-babel,file-loader,postcss-cssnext,postcss-discard-comments,postcss-global-import,postcss-import,postcss-inline-svg,postcss-loader,postcss-modules-values,postcss-sass-color-functions,precss,style-loader,url-loader,webpack-dev-server",
    "eslint-fix": "eslint src/** --fix",
    "eslint": "eslint src/**",
    "start:break": "nodemon -w src/server -w src/shared -w config -w package.json index.js --exec 'babel-node --inspect=8009 --debug-brk'",
    "start:debug": "nodemon -w src/server -w src/shared -w config -w package.json index.js --exec 'babel-node --inspect=8009'",
    "start": "babel-node index.js",
    "start:watch": "nodemon -w src/server -w src/shared -w config -w package.json index.js --exec 'babel-node'",
    "test": "npm run eslint && npm run depcheck",
    "wds": "webpack-dev-server --progress --config ./config/webpack/client.babel.js"
  },
  "config": {
    "cssClassPattern": "[name]_[local]__[hash:base64:5]'"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "css-modules-require-hook": "^4.0.5",
    "express": "^4.14.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "depcheck": "^0.6.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.11.2",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-global-import": "^1.0.0",
    "postcss-import": "^10.0.0",
    "postcss-inline-svg": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-modules-values": "^1.2.2",
    "postcss-sass-color-functions": "^1.1.0",
    "precss": "^1.4.0",
    "react-hot-loader": "3.0.0-beta.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.3.0"
  }
}
